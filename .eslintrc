{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "ecmaFeatures": {
    "jsx": true
  },
  "env": {
    "es6": true
  },
  "plugins": [
    "react",
    "jsx-a11y"
  ],
  "rules": {
    "no-use-before-define": [0],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
  },
  "globals": {
    "require": false,
    "it": true,
    "expect": true,
    "describe": true,
    "fetch": true,
    "FormData": true,
    "__DEV__": true,
    "navigator": true
  }
}
